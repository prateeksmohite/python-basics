marks = [94.4, 87.5, 95.2, 66.4, 45.1]
print(marks)
print(type(marks))
print(len(marks))
print(marks[0])
print(marks[1])

#strings - immutable means couldn't be changed
#lists  - mutable means can be changed

student = ["virat", 95.4, 17, "delhi"]
print(student)
print(student[0])
student[0] = "kohli"        #in lists we can do the changes and replace the things that we want but in strings we can not do that
print(student)